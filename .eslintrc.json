{
  "env": {
      "browser": true,
      "commonjs": true,
      "es6": true,
      "mocha": true
  },
  "extends": "airbnb-base",
  "globals": {
      "Atomics": "readonly",
      "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
      "ecmaVersion": 2018
  },
  "rules": {
      "no-unsafe-negation": 0,
      "consistent-return": 0,
      "class-methods-use-this": 0
  }
}